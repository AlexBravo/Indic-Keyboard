<merge
    xmlns:latin="http://schemas.android.com/apk/res-auto"
 >
    <switch>
        <case
            latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted"
            >
            <Key
                latin:keyLabel="&#x0C1E;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keyLabel="&#x0C28;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keyLabel="&#x0C01;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keyLabel="&#x0C35;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keyLabel="&#x0C31;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keyLabel="&#x0C33;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keyLabel="&#x0C37;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keyLabel="&#x0C39;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />
            <Key
                latin:keyLabel="&#x0C59;"
                latin:keyLabelFlags="fontNormal" />
        </case>

        <default>
            <Key
                latin:keyLabel="&#x0C19;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keyLabel="&#x0C23;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keyLabel="&#x0C2E;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />
            <Key
                latin:keyLabel="&#x0C2F;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />
            <Key
                latin:keyLabel="&#x0C30;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keyLabel="&#x0C32;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keyLabel="&#x0C36;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keyLabel="&#x0C38;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />
            <Key
                latin:keyLabel="&#x0C58;"
                latin:keyLabelFlags="fontNormal" />
        </default>
    </switch>
</merge>
